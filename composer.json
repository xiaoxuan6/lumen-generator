{
    "name": "james.xue/lumen-generator",
    "license": "MIT",
    "homepage": "https://github.com/xiaoxuan6/lumen-generator.git",
    "description": "文件生成器",
    "authors": [
        {
            "name": "james.xue",
            "email": "1527736751@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^11.15.0",
        "illuminate/filesystem": "^5.0||^6.0||^7.0||^8.0||^9.0",
        "illuminate/console": "^5.0||^6.0||^7.0||^8.0||^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vinhson\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vinhson\\LumenGeneratorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "scripts": {
        "fix-style": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --ansi"
    }
}
